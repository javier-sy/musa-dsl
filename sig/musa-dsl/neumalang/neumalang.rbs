# RBS for neumalang/neumalang.rb - Neumalang parser

module Musa
  module Neumalang
    # Neumalang parser for parsing neuma text notation.
    module Neumalang
      extend Neumalang

      # Parses Neumalang notation string or file.
      def parse: (
        String | File string_or_file,
        ?decode_with: untyped?,
        ?debug: bool?
      ) -> (Musa::Series::_SerieProtocol | Array[Hash[Symbol, untyped]])

      # Parses Neumalang notation file.
      def parse_file: (
        String filename,
        ?decode_with: untyped?,
        ?debug: bool?
      ) -> (Musa::Series::_SerieProtocol | Array[Hash[Symbol, untyped]])

      # Parser namespace containing grammar and semantic modules.
      module Parser
        # Grammar namespace populated by Citrus.
        module Grammar
          # Grammar class with parse methods.
          class Grammar
            def self.parse: (String input) -> untyped
          end
        end
      end
    end
  end
end
